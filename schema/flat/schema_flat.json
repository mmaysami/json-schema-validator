{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "Estimates Schema",
    "required": [
      "CountryOfField",
      "CountryDesc",
      "CategoryCode",
      "VersionDate",
			"Method"
    ],
    "properties": {
      "CountryOfField": {
        "$id": "#/properties/CountryOfField",
        "type": "string",
        "title": "The Countryoffield Schema",
        "default": "",
        "examples": [
          "USA___"
        ],
        "maxLength": 6,
        "pattern": "^(.*)$"
      },
      "CountryDesc": {
        "$id": "#/properties/CountryDesc",
        "type": "string",
        "title": "The Countrydesc Schema",
        "default": "",
        "examples": [
          "United States"
        ],
        "pattern": "^(.*)$"
      },
      "CategoryCode": {
        "$id": "#/properties/CategoryCode",
        "type": "string",
        "title": "The Categorycode Schema",
        "default": 0,
        "changeme": 0,
        "maxLength": 6
      },
      "VersionDate": {
        "$id": "#/properties/VersionDate",
        "type": "string",
        "title": "The Versiondate Schema",
        "default": 0,
        "examples": [
          20160319
        ],
        "maxLength": 8
      },
      "Method": {
        "$id": "#/properties/Method",
        "type": "string",
        "title": "The Method Schema",
        "default": "",
        "examples": [
          "M_00"
        ],
        "pattern": "^(M_00|M_00_RPA)$"
      }
    }
  }